<!-- all Live changes from Sockets goes here -->
<script>
    $(document).ready(function(){
          var socket = io();

          $("#click").click(function(){
            socket.emit('status added',$("#comment").val());
          });
          
          socket.on('logs',function(msg){
            $('#logs').append('<samp>' + msg + '</samp><br>');
          });
    });
</script>